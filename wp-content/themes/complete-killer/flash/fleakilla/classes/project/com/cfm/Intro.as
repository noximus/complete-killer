package com.cfm{	import flash.display.*;	import flash.events.*;		public class Intro extends MovieClip	{		public var introBtn : MovieClip;		private var _model:AppModel;		public var ref:String;				public function Intro():void		{			_model = AppModel.getInstance();			introBtn.buttonMode = true;			introBtn.addEventListener(MouseEvent.MOUSE_DOWN,_handleMouse,false,0,true);		}				private function _handleMouse(e:MouseEvent):void		{			_model.trackPage("ent/flea_killah/begin");			_model.requestPage("capture");		}	}}